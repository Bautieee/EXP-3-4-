
#include <iostream>
#include <algorithm>
using namespace std;


void print(char arr[], int n)
{
	for (int i = 0; i < n; i++) {
		cout << arr[i] << " ";
	}
}

//broken vow :((( ma'am suko na kooo sorry 
void reverse(char arr[], int n)
{
	reverse(arr, arr + n);
}

int main()
{
	char arr[] = { 'a','9', 'n', '3', '5', 'z'};
	int n = sizeof(arr)/sizeof(arr[0]);

	reverse(arr, n);
	print(arr, n);

	return 0;
}
